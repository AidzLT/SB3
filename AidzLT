local SOMEXHUB = loadstring(game:HttpGet("https://raw.githubusercontent.com/AidzLT/ProjectSlayer1/main/AntiCheatTrash1"))();
local xNero = SOMEXHUB:Create({Name = "SB3",Size = UDim2.fromOffset(100, 300),Theme = SOMEXHUB.Themes.Dark,Link = "https://github.com/facebook.comAidzLT"})

xNero:Notification({Title = "Welcome AidzLT Community",Text = "game này anticheat như cặc",Duration = 3})
xNero:Credit{Name = "LuongTruong",Description = "Discord AidzLT",}

local Main = xNero:Tab({Name = "Aidz Script",Icon = "http://www.roblox.com/asset/?id=6414160239"})

Main:Button({Name = "antilag",Description = nil,Callback = function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/AidzLT/antilag/main/univers"))()
end})

Main:Button({Name = "Kill Aura",Description = nil,Callback = function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/x3fall3nangel/FallAngelHub/main/Main.lua"))()
end})

Main:Button({Name = "Inf Yiel",Description = nil,Callback = function()
	loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end})

Main:Button({Name = "HopServer",Description = nil,Callback = function()
	local module = loadstring(game:HttpGet"https://raw.githubusercontent.com/LeoKholYt/roblox/main/lk_serverhop.lua")()

module:Teleport(game.PlaceId)
end})

Discord:Button({Name = "Copy Discord",Description = nil,Callback = function()
xNero:Notification({Title = "Discord AidzLT",Text = "https://discord.gg/cocailon",Duration = 1})
setclipboard("https://discord.gg/cocailon")
end})
